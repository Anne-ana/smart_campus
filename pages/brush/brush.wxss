/* pages/brush/brush.wxss */
.scroll-y{
  height: 88vh;
}
/* 头部 */
.box{
  height: 400rpx;
}
.box>image:nth-child(1){
  width: 40rpx;
  height: 40rpx;
  margin-left: 40rpx;
}
.box>image:nth-child(2){
  width: 80rpx;
  height: 80rpx;
  margin-left: 40rpx;
  margin-top: 25rpx;
  display: flex;
}
.box>view:nth-child(3){
  font-size: 40rpx;
  font-weight: 600;
  margin-left: 40rpx;
  margin-top: 25rpx;
}
.box>view:nth-child(4){
  font-size: 25rpx;
  margin-top: 25rpx;
  margin-left: 40rpx;
  color:gray;
}
/* 人脸识别 */
.box1{
  width: 100%;
  height: 400rpx;
}
.box1>image{
  height: 400rpx;
  margin-left: 70rpx;
  margin-top: -60rpx;
}
.box1>view{
  font-size: 25rp;
  margin-left: 250rpx;
  color:gray;
}
/* 导航栏 */
.bar{
  width: 90%;
  margin: auto;
  display: flex;
  justify-content: space-around;
}

.bar>view>view>image{
  width: 50rpx;
  height: 50rpx;
  margin-top: 250rpx;
}
.bar>view>view{
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}